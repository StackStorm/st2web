    "react-textarea-autosize": "5.2.1",
    "validator": "5.7.0"
  },
  "devDependencies": {
    "@stackstorm/browserify-postcss": "0.3.4-patch.5",
    "babelify": "8.0.0",
    "postcss": "6.0.22",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "postcss-preset-env": "5.1.0"
